raw_data = csvread('../data/Walking_1455415552.csv');
cur = raw_data(1, 11);
max_time = raw_data(end, 11);
[m, n] = size(raw_data);

last_i = 1;
processed_means = zeros(floor((max_time - cur) / 4), 10);
processed_variance = zeros(floor((max_time - cur) / 4), 10);
processed_max = zeros(floor((max_time - cur) / 4), 10);
processed_zeros = zeros(floor((max_time - cur) / 4), 11);
j=1;
for i = 1:m
   if (raw_data(i, 11) - cur > 4)
       cur_data = raw_data(last_i: i-1, 1:end-1);
       processed_means(j, :) = mean(cur_data, 1);
       j = j + 1;
   end
end